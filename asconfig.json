{
  "targets": {
    "debug": {
      "outFile": "build/debug.wasm",
      "textFile": "build/debug.wat",
      "sourceMap": false,
      "debug": true
    },
    "release": {
      "outFile": "build/release.wasm",
      "textFile": "build/release.wat",
      "sourceMap": false,
      "optimizeLevel": 3,
      "shrinkLevel": 3,
      "converge": false,
      "noAssert": false
    }
  },
  "options": {
    "bindings": "raw"
  },
  "features": {
    "runtime": "stub"
  }
}